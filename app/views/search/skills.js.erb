var searchString = '<%= @search_string %>';
var results = <%=raw @skills.to_json %>;

var container = $('#skill-auto-complete');
container.html('<%= j(render "search/skill_auto_complete_results") %>');

if(results && results.length > 0) {
	var list = container.find('li');
	var inputTextField = $('input#compliment_skill_id');
	var inputIdField = $('input#skill_id_result')
	container.show();
	ListNavigator.init(list, inputTextField, inputIdField, container);
} else {
	container.hide();
}