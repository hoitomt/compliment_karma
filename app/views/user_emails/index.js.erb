var source = '<%= @source %>';
var userId = '<%= @user.id %>';
console.log(source);
var view = '<%= j(render :partial => "user_emails/add_manage_emails", 
										     :locals => {:user_emails => @user_emails, 
										 						     :user_email => @user_email,
										 						     :user => @user} ) %>';
var redirectView = '<%=j render(:partial => "users/fancyboxes/unconfirmed",
                                :locals => {:user => @user}) %>';
var host = 'http://www.complimentkarma.com/users'
<% if Rails.env.development? %>
  host = 'http://localhost:3000/users/'
<% end %>
var options = {
  view: view,
  source: source,
  userId: userId,
  redirectView: redirectView,
  redirectHost: host
};
console.log(options);
ShowFancybox.init(options);
InlineForms.init();
