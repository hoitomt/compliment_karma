console.log("Handle Event");
var updateNewCompliment = function() {
	$('input#compliment_receiver_email').val('<%=j @recipient_email %>');
	$.fn.inputStyle();
	ComplimentUI.flashBlue();
}
console.log("Handle Event 2");
var popupOpen = $('.fancybox-skin').is(':visible');
if(popupOpen) {
	console.log("Close Fancybox");
	$.fancybox.close();
	setTimeout(updateNewCompliment, 750);
} else {
	console.log("no fancybox to close");
	updateNewCompliment();
}
